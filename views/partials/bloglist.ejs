<p><%= blogCollection.length %> </p>
<% if (blogCollection.length > 0){ %>
    <% blogCollection.forEach((blog) => { %> 
        <p><%= moment(blog.createdAt).format("ll") %></p> 
            <a href="/">
                <h1><%= blog.title %></h1> 
                <p><%= blog.display %></p> 
                <p><%= blog.body %></p> 
            </a>
    <% }) %>  
    <%} else {%>
        <p>no blogs yet...</p>
        <% } %>